{
  "version": 4,
  "configurations": [
    {
      "name": "qnx",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "compilerPath": "${env:QNX_HOST}/usr/bin/qcc",
      "defines": [
        "${default}"
      ],
      "forcedInclude": [
        ".vscode/preinclude.h"
      ],
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "${default}",
      "configurationProvider": "qnx.qnx-vscode"
    }
  ]
}